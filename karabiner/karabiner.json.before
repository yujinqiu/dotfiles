                            },
                            {
                                "from": {
                                    "key_code": "j",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_command",
                                            "left_control",
                                            "left_option",
                                            "left_shift"
                                        ],
                                        "optional": [
                                            "any"
                                        ]
